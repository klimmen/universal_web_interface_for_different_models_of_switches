<p id="notice"><%= notice %></p>

<h1>Listing Switch Models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @switch_models.each do |switch_model| %>
      <tr>
        <td><%= switch_model.name %></td>
        <td><%= link_to 'Show', switch_model %></td>
        <td><%= link_to 'Edit', edit_switch_model_path(switch_model) %></td>
        <td><%= link_to 'Destroy', switch_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Switch model', new_switch_model_path %>
